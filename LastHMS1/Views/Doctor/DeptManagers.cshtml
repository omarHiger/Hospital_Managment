@model IEnumerable<LastHMS1.Models.Doctor>

@{
    ViewData["Title"] = "DeptManagers";
}

<h1>DeptManagers</h1>

<p>
    <a asp-controller="Doctor" asp-action="CreateDeptManager" asp-route="@ViewBag.HospitalId">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_First_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Middle_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Last_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_National_Number)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Gender)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Area)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Street)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_City)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Password)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Family_Members)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Specialization)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Qualifications)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Social_Status)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Birth_Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Birth_Place)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Doctor_Hire_Date)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_First_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Middle_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Last_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_National_Number)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Gender)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Area)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Street)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_City)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Password)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Family_Members)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Specialization)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Qualifications)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Social_Status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Birth_Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Birth_Place)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Doctor_Hire_Date)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Doctor_Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Doctor_Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Doctor_Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
